<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bwie.three">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".net.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".view.SearchActivity" />
        <activity android:name=".view.GoodsActivity" />
        <activity android:name=".view.CartActivity" />
        <activity android:name=".view.HelloActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ConfirmActivity" />
        <activity android:name=".view.MyOrderActivity"></activity>
    </application>

</manifest>